{
  "name": "@bucketco/tracking-sdk",
  "version": "1.3.1",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc && webpack",
    "test": "vitest",
    "test:ci": "vitest --reporter=junit --outputFile=junit.xml",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:ci": "eslint --output-file eslint-report.json --format json .; exit 0",
    "prettier": "prettier --check .",
    "format": "yarn prettier --write && yarn lint --fix"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/bucket-tracking-sdk.node.js",
  "browser": "./dist/bucket-tracking-sdk.browser.js",
  "types": "./dist/types/src/index.d.ts",
  "devDependencies": {
    "@types/node": "^18.16.20",
    "@types/webpack": "^5.28.0",
    "@types/webpack-node-externals": "^3.0.0",
    "c8": "^8.0.1",
    "@eslint/js": "^8.47.0",
    "globals": "^13.21.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "nock": "^13.3.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.2",
    "vitest": "^0.29.8",
    "webpack": "^5.70.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "prettier": "^3.0.2"
  },
  "dependencies": {
    "ably": "^1.2.43",
    "cross-fetch": "^4.0.0",
    "is-bundling-for-browser-or-node": "^1.1.1"
  }
}
