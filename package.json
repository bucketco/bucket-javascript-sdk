{
  "name": "@bucketco/tracking-sdk",
  "version": "1.4.1-0",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/bucketco/bucket-tracking-sdk.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && webpack",
    "test": "vitest -c vite.config.js",
    "test:e2e": "vitest run -c vite.e2e.config.js && playwright test",
    "test:ci": "vitest run -c vite.config.js && vitest run -c vite.e2e.config.js && CI=true playwright test",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:ci": "eslint --output-file eslint-report.json --format json .",
    "prettier": "prettier --check .",
    "format": "yarn prettier --write && yarn lint --fix",
    "preversion": "yarn lint && yarn prettier && yarn vitest run && yarn build"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/bucket-tracking-sdk.node.js",
  "browser": "./dist/bucket-tracking-sdk.browser.js",
  "types": "./dist/types/src/index.d.ts",
  "devDependencies": {
    "@eslint/js": "^8.47.0",
    "@playwright/test": "^1.37.1",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^18.16.20",
    "@types/webpack": "^5.28.1",
    "@types/webpack-node-externals": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitest/coverage-c8": "^0.33.0",
    "c8": "^8.0.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "globals": "^13.21.0",
    "http-server": "^14.1.1",
    "jsdom": "^22.1.0",
    "nock": "^13.3.3",
    "prettier": "^3.0.2",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.2",
    "vitest": "^0.30.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "ably": "^1.2.43",
    "cross-fetch": "^4.0.0",
    "is-bundling-for-browser-or-node": "^1.1.1",
    "js-cookie": "^3.0.5"
  }
}
