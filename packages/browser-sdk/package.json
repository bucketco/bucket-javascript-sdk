{
  "name": "browser-sdk",
  "version": "0.0.1",
  "packageManager": "yarn@4.1.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bucketco/bucket-javascript-sdk.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --project tsconfig.build.json && webpack",
    "test": "vitest -c vite.config.js",
    "test:ci": "vitest run -c vite.config.js --reporter=default --reporter=junit --outputFile=junit.xml",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:ci": "eslint --output-file eslint-report.json --format json .",
    "prettier": "prettier --check .",
    "format": "yarn lint --fix && yarn prettier --write",
    "preversion": "yarn lint && yarn prettier && yarn vitest run -c vite.config.js && yarn build"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/bucket.browser-sdk.js",
  "module": "./dist/bucket.browser-sdk.module.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/bucket.browser-sdk.module.js"
    }
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.8",
    "canonical-json": "^0.0.4",
    "js-cookie": "^3.0.5",
    "preact": "^10.22.1"
  },
  "devDependencies": {
    "@bucketco/eslint-config": "0.0.2",
    "@bucketco/tsconfig": "0.0.2",
    "@types/node": "^20.14.0",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.7.0",
    "jsdom": "^24.1.0",
    "postcss": "^8.4.39",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.0.13",
    "vitest": "^1.6.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
