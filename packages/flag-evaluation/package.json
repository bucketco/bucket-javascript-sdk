    "@types/node": "^22.12.0",
    "eslint": "^9.21.0",
    "prettier": "^3.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vitest": "^2.0.5"
  },
  "dependencies": {
